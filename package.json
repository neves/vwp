{
  "name": "vwp",
  "version": "0.2.0",
  "description": "Webpack Vue.js build tool",
  "main": "index.js",
  "scripts": {
    "test": "cd test && ../bin/vwp build $@",
    "upgrade:check": "ncu",
    "upgrade:apply": "ncu -a",
    "deploy": "npm version patch && git push --follow-tags && npm publish"
  },
  "keywords": [
    "webpack",
    "vue",
    "build",
    "tool"
  ],
  "author": "Marcos Neves <marcos.neves@gmail.com> (http://neves.github.io/)",
  "license": "MIT",
  "bin": "./bin/vwp",
  "files": [
    "bin/",
    "*.js"
  ],
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "coffee-loader": "^0.8.0",
    "coffee-script": "^1.12.7",
    "copy-webpack-plugin": "^4.2.0",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "json-loader": "^0.5.7",
    "less": "^3.0.0-alpha.3",
    "less-loader": "^4.0.5",
    "node-sass": "^4.5.3",
    "pug": "2.0.0-rc.4",
    "pug-loader": "^2.3.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.6.2",
    "vue": "^2.5.2",
    "vue-loader": "^13.3.0",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3",
    "wpc": ">=0.2.0",
    "yml-loader": "^2.1.0"
  },
  "devDependencies": {
    "npm-check-updates": "^2.13.0"
  }
}
